{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "outline-document-search",
  "title": "Outline Document Search",
  "description": "Search documents in multiple instances of Outline.",
  "icon": "outline.png",
  "author": "dnlfrst",
  "contributors": [
    "derFrisson"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-outline-documents",
      "title": "Search Documents",
      "subtitle": "Outline",
      "description": "Search documents in multiple instances of Outline.",
      "mode": "view"
    },
    {
      "name": "starred-documents",
      "title": "Starred Documents",
      "subtitle": "Outline",
      "description": "View your starred documents.",
      "mode": "view"
    },
    {
      "name": "recent-documents",
      "title": "Recent Documents",
      "subtitle": "Outline",
      "description": "View recently accessed documents.",
      "mode": "view"
    },
    {
      "name": "browse-collections",
      "title": "Browse Collections",
      "subtitle": "Outline",
      "description": "Browse documents by collection.",
      "mode": "view"
    },
    {
      "name": "create-document",
      "title": "Create Document",
      "subtitle": "Outline",
      "description": "Create a new document in Outline.",
      "mode": "view"
    },
    {
      "name": "manage-outline-instances",
      "title": "Manage Instances",
      "subtitle": "Outline",
      "description": "Manage instances of Outline.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "node-fetch": "^3.3.2",
    "react-use": "^17.5.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@raycast/utils": "^1.15.0",
    "@types/node": "20.8.10",
    "@types/node-fetch": "^2.6.9",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}